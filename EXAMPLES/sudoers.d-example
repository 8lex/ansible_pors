# PORS starts as a separate user in multi-user environments
# all users defined in the User_Alias are allowed to run PORS

User_Alias      USERAE=foo,bar,moo
Cmnd_Alias      AECMD = /usr/bin/env CALLUSR=* /opt/ansible_pors/pors *,/usr/bin/env CALLUSR=* ./pors *

# change the user in ALL=(splunk) depending on the variable AEUSER in PORS
USERAE     	ALL=(splunk)      NOPASSWD: AECMD
